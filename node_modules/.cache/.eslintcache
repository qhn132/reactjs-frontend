 signUp(data)\r\n            .then(dataInput => {\r\n                if (dataInput.error) {\r\n                    setError(dataInput.error);\r\n                } else {\r\n                    e.target.reset()\r\n                    setError(\"\");\r\n                    setSuccess(true)\r\n                }\r\n            })\r\n\r\n    }\r\n    const showError = () => {\r\n        return <div className=\"alerl alerl-danger\" style={{ display: error ? \"block\" : \"none\" }}>\r\n            {error}\r\n        </div>\r\n    }\r\n    const showSucces = () => {\r\n        return <div className=\"alerl alerl-info\" style={{ display: success ? \"block\" : \"none\" }}>\r\n            bạn đã tạo tài khoản thành công . Click để <Link to=\"/signin\">đăng nhập</Link>\r\n        </div>\r\n    }\r\n    const signUpForm = () => {\r\n        return (\r\n            <form onSubmit={handleSubmit(onSubmit)}>\r\n                <div className=\"mb-3\">\r\n                    <label htmlFor=\"name\" className=\"form-label\">Tên đăng nhập</label>\r\n                    <input type=\"text\"\r\n                        className=\"form-control\"\r\n                        id=\"name\"\r\n                        {...register('name')}\r\n                    />\r\n                </div>\r\n                <div className=\"mb-3\">\r\n                    <label htmlFor=\"email\" className=\"form-label\">Email</label>\r\n                    <input type=\"email\"\r\n                        className=\"form-control\"\r\n                        id=\"email\"\r\n                        {...register('email')}\r\n                    />\r\n                </div>\r\n                <div className=\"mb-3\">\r\n                    <label htmlFor=\"password\" className=\"form-label\">Mật khẩu</label>\r\n                    <input type=\"password\"\r\n                        className=\"form-control\"\r\n                        id=\"password\"\r\n                        {...register('password')}\r\n                    />\r\n                </div>\r\n                <button className=\"btn btn-primary\">Đăng ký</button>\r\n\r\n            </form>\r\n        )\r\n    }\r\n\r\n    return (\r\n        <div>\r\n            {showError()}\r\n            {showSucces()}\r\n            {signUpForm()}\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default SignUp\r\n","/home/qhn132/Documents/reactjs/mern-ecommerce-fe/src/config.js",[],"/home/qhn132/Documents/reactjs/mern-ecommerce-fe/src/api/categoryAPI.js",[],"/home/qhn132/Documents/reactjs/mern-ecommerce-fe/src/admin/category/add.js",[],"/home/qhn132/Documents/reactjs/mern-ecommerce-fe/src/admin/category/edit.js",["153","154"],"import React, { useEffect, useState } from 'react'\nimport { set, useForm } from 'react-hook-form'\nimport { useHistory, useParams } from 'react-router-dom'\nimport CategoryAPI from '../../api/categoryAPI'\n\nconst AdminCateUpdatePage = ({ onCateUpdate }) => {\n    const { register, handleSubmit, reset, formState: { errors } } = useForm();\n    const history = useHistory();\n    const [category, setCategory] = useState({});\n    let { id } = useParams();\n    useEffect(() => {\n        // call API\n        const getCategories = async () => {\n            try {\n                const { data } = await CategoryAPI.get(id);\n                setCategory(data)\n                reset(data)\n            } catch (error) {\n                console.log(error)\n            }\n        }\n        getCategories();\n    }, [])\n    const onHandleSubmit = (data) => {\n        console.log(data)\n        const uploads = new FormData();\n        uploads.append(\"name\", data.abc);\n        // uploads.append(\"photo\", data.image[0]);\n        onCateUpdate(id, uploads);\n        history.push('/admin/category');\n    }\n\n    return (\n        <div>\n            <h1>Cate update</h1>\n            <form onSubmit={handleSubmit(onHandleSubmit)}>\n                <div className=\"form-floating mb-3\">\n                    <input typ